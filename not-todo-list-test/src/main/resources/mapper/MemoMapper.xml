<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.nottodolisttest.memo.MemoMapper">

    <select id="selMemo">
        select content
        from t_memo
    </select>

    <update id="updMemo">
        update t_memo
        set content = #{content}
    </update>

</mapper>